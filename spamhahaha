repeat wait()
until game:IsLoaded()
local TableChat = {"SPAM","SPAM","SPAM","SPAM","SPAM","SPAM","SPAM","SPAM","SPAM","SPAM","SPAM","SPAM","SPAM","SPAM","SPAM","SPAM","SPAM","SPAM","SPAM","SPAM","SPAM","SPAM"}
spawn(function()
    while wait() do
        pcall(function()
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(TableChat[math.random(1,#TableChat)],"All")
            wait(45)
        end)
    end
end)
